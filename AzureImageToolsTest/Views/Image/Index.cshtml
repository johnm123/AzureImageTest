
    @{
        ViewBag.Title = "Azure Test";
    }

    <div class="jumbotron">
        <h3>Azure Test</h3>
    </div>

    <div class="row">

    </div>

    <div class="body-content">
        <form action="@Url.Action("Upload", "Image")" method="post" enctype="multipart/form-data">

            <p>
                <label for="inputFile">Upload your file here</label>
                <input type="file" name="inputFile" id="inputFile" />
                <input type="submit" value="Upload" />
            </p>
        </form>
        @foreach(var uri in Model.UriStrings)
        {
            <img alt="No image." src="@uri" />
        }
    </div>
